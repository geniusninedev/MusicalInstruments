<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.geniusnine.android.musicalinstruments">

    <application
        android:allowBackup="true"
        android:icon="@drawable/m"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Instruments.MusicInstruweb"
            android:label="Instrument Details"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".Intro"
            android:label="Introduction"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".HomeMade"
            android:label="Home Made Musical Instruments"
            android:parentActivityName=".MainActivity"></activity>
    </application>

</manifest>